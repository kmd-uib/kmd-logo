    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "strict": true,
    "declaration": true,
    "declarationDir": "./dist",
    "emitDeclarationOnly": true,
    "rootDir": "./src"
  },
  "include": [
    "src/lib/index.ts",
    "src/Logo/index.tsx",
    "src/Logo/svgs.tsx",
    "src/Logo/stepper.ts"
  ]
}
